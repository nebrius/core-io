{
  "name": "core-io",
  "version": "3.0.0",
  "description": "The core library for Raspi IO",
  "main": "dist/index.js",
  "types": "./declarations/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/nebrius/raspi-io-core.git"
  },
  "homepage": "https://github.com/nebrius/raspi-io-core",
  "keywords": [
    "raspberrypi",
    "nodebots",
    "robots"
  ],
  "dependencies": {
    "abstract-io": "^1.5.0",
    "core-io-types": "^2.2.0"
  },
  "devDependencies": {
    "@types/jasmine": "^3.3.5",
    "@types/node": "^10.12.12",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "istanbul": "^0.4.5",
    "jasmine": "^3.3.0",
    "raspi": "^6.0.0",
    "raspi-board": "^7.1.0",
    "tslint": "^5.11.0",
    "typescript": "^3.2.2",
    "typescript-tslint-plugin": "^0.2.1"
  },
  "scripts": {
    "prebuild": "tslint 'src/**/*.ts'",
    "lint": "tslint --project tslint.json",
    "build": "tsc --build ts-src-config.json",
    "build-tests": "tsc --build ts-test-config.json",
    "test": "cross-env RASPI_IO_TEST_MODE=true istanbul cover -x dist-spec/spec/mocks.js ./node_modules/jasmine/bin/jasmine.js --captureExceptions dist-spec/spec/**/*.spec.js && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "test-local": "cross-env RASPI_IO_TEST_MODE=true jasmine --random=false dist-spec/spec/**/*.spec.js"
  },
  "engines": {
    "node": ">= 6.4.0"
  },
  "author": "Bryan Hughes <bryan@nebri.us>",
  "contributors": [
    {
      "name": "Bryan Hughes",
      "email": "bryan@nebri.us"
    },
    {
      "name": "Brian Cooke",
      "email": "bdscooke@gmail.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nebrius/raspi-io-core/issues"
  }
}
